{
  "name": "{{ registry_name | default('registry2') }}",
  "credentialsId": "{{ credentials_id | default('registry2-credentials') }}",
  "mavenConfig": {
    "repositoryDomainName": "{{ maven_domain | default('maven2.example.com') }}",
    "fullRepositoryUrl": "{{ maven_url | default('https://maven2.example.com/repository') }}",
    "targetSnapshot": "{{ maven_snapshot | default('snapshots') }}",
    "targetStaging": "{{ maven_staging | default('staging') }}",
    "targetRelease": "{{ maven_release | default('releases') }}"
  },
  "dockerConfig": {
    "snapshotUri": "{{ docker_snapshot_uri | default('docker2.example.com/snapshot') }}",
    "stagingUri": "{{ docker_staging_uri | default('docker2.example.com/staging') }}",
    "releaseUri": "{{ docker_release_uri | default('docker2.example.com/release') }}",
    "groupUri": "{{ docker_group_uri | default('docker2.example.com/group') }}",
    "snapshotRepoName": "{{ docker_snapshot_repo | default('docker2-snapshot') }}",
    "stagingRepoName": "{{ docker_staging_repo | default('docker2-staging') }}",
    "releaseRepoName": "{{ docker_release_repo | default('docker2-release') }}",
    "groupName": "{{ docker_group_name | default('docker2-group') }}"
  },
  "helmConfig": {
    "snapshotUri": "{{ helm_snapshot_uri | default('helm2.example.com/snapshot') }}",
    "stagingUri": "{{ helm_staging_uri | default('helm2.example.com/staging') }}",
    "releaseUri": "{{ helm_release_uri | default('helm2.example.com/release') }}",
    "groupUri": "{{ helm_group_uri | default('helm2.example.com/group') }}",
    "snapshotRepoName": "{{ helm_snapshot_repo | default('helm2-snapshot') }}",
    "stagingRepoName": "{{ helm_staging_repo | default('helm2-staging') }}",
    "releaseRepoName": "{{ helm_release_repo | default('helm2-release') }}",
    "groupName": "{{ helm_group_name | default('helm2-group') }}"
  }
}
