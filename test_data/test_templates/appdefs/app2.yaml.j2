# AppDef template for app2
name: {{ app_name | default('app2') }}
registryName: {{ registry_name | default('registry2') }}
artifactId: {{ artifact_id | default('com.example.app2') }}
groupId: {{ group_id | default('com.example') }}
supportParallelDeploy: {{ support_parallel_deploy | default(true) | lower }}
deployParameters:
  param1: {{ deploy_param1 | default('app2_default_value1') }}
  param2: {{ deploy_param2 | default('app2_default_value2') }}
  env: {{ env_name | default('dev') }}
  cluster: {{ cluster_name | default('default-cluster') }}
technicalConfigurationParameters:
  tech_param1: {{ tech_param1 | default('app2_default_tech_value1') }}
  tech_param2: {{ tech_param2 | default('app2_default_tech_value2') }}
  version: {{ version | default('1.0.0') }}
solutionDescriptor: {{ solution_descriptor | default(false) | lower }}
